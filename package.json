{
  "name": "genqlx",
  "version": "3.0.0",
  "private": true,
  "workspaces": [
    "cli",
    "runtime",
    "website",
    "example-usage",
    "try-clients",
    "integration-tests"
  ],
  "scripts": {
    "buildall": "ultra -r --filter '@genqlx/*' --topology build",
    "examplecli": "yarn play -o example/generated -e https://graphql-compose.herokuapp.com/northwind/",
    "genall": "ultra -r gen",
    "integration": "cd integration-tests && yarn gen && tsc && yarn test",
    "play": "node -r sucrase/register cli/src/cli.ts",
    "publ": "yarn buildall && lerna publish minor",
    "runexample": "node -r sucrase/register example/example.ts",
    "test": "ultra -r test",
    "tscall": "ultra -r --filter '@genqlx/*' tsc",
    "watch": "ultra -r watch"
  },
  "devDependencies": {
    "lerna": "4.0.0",
    "typescript": "4.7.2",
    "ultra-runner": "3.10.5"
  },
  "resolutions": {
    "graphql": "^16.5.0",
    "ws": "^8.6.0"
  }
}
