{
  "name": "@gqlts/cli",
  "version": "3.2.10",
  "description": "Generate a client sdk from your GraphQl API",
  "keywords": [
    "gqlts",
    "graphql",
    "axios",
    "typescript",
    "types-graphql",
    "types-graphql-client",
    "types-graphql-axios",
    "graphql-sdk",
    "graphql-sdk-generator",
    "graphql-sdk-cli",
    "graphql-subscriptions",
    "graphql-subscriptions-client",
    "graphql-typescript",
    "graphql-axios-client",
    "graphql-typescript-client",
    "graphql-typescript-sdk"
  ],
  "repository": {
    "url": "https://github.com/meabed/gqlts"
  },
  "license": "MIT",
  "author": "Meabed <mo@meabed.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gqlts": "./dist/cli.js"
  },
  "files": [
    "/dist/*",
    "/esm/*",
    "src"
  ],
  "scripts": {
    "build": "rm -rf dist esm *.tsbuildinfo && tsc",
    "compile": "sucrase -q ./src -d ./dist --transforms typescript,imports",
    "start": "tsdx watch",
    "test": "jest",
    "watch": "tsc -w"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/_old/**",
      "!**/*.case.ts",
      "!src/testHelpers/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "mocha": {
    "require": "tests/init.js",
    "spec": "tests/**.ts",
    "timeout": 9999999999
  },
  "dependencies": {
    "@gqlts/runtime": "^3.2.10",
    "@graphql-tools/graphql-file-loader": "^7.5.5",
    "@graphql-tools/load": "^7.7.7",
    "chalk": "^4.1.2",
    "fs-extra": "^10.1.0",
    "graphql": "*",
    "isomorphic-unfetch": "^3.1.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "prettier": "^2.7.1",
    "qs": "^6.11.0",
    "rimraf": "^3.0.2",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "3.0.0",
    "@types/common-tags": "1.8.1",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "29.2.0",
    "@types/listr": "0.14.4",
    "@types/lodash": "4.14.186",
    "@types/mkdirp": "1.0.2",
    "@types/node": "18.8.3",
    "@types/prettier": "2.7.1",
    "@types/qs": "6.9.7",
    "@types/rimraf": "3.0.2",
    "@types/yargs": "17.0.13",
    "common-tags": "1.8.2",
    "dotenv": "16.0.3",
    "husky": "8.0.1",
    "jest": "29.2.1",
    "pretty-quick": "3.1.3",
    "sucrase": "3.28.0",
    "ts-jest": "29.0.3"
  },
  "_": "[bump]"
}
